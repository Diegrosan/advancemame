# Checks for header files.
# Checks for typedefs, structures, and compiler characteristics.
# Checks for library functions.

# Checks for optimizations
# Check whether --enable-debug or --disable-debug was given.
if test "${enable_debug+set}" = set; then
  enableval="$enable_debug"
  ac_enable_debug=$enableval
else
  ac_enable_debug=no

fi;
CONF_DEBUG=$ac_enable_debug


if test $ac_auto_cflags = yes; then
	if test $ac_enable_debug = yes ; then
		CFLAGS="-O0 -g -Wall -Wno-sign-compare"
	else
		CFLAGS=""

if grep -i "family.*:.*3" /proc/cpuinfo > /dev/null ; then
	ac_cpu_arch="i386"
	ac_cpu_family=3
elif grep -i "family.*:.*4" /proc/cpuinfo > /dev/null ; then
	ac_cpu_arch="i486"
	ac_cpu_family=4
elif grep -i "family.*:.*5" /proc/cpuinfo > /dev/null ; then
	if grep -i "model name.*:.*K6" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="k6"
	elif grep -i "model name.*:.*Pentium MMX" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentium-mmx"
	elif grep -i "model name.*:.*Pentium" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentium"
	else
		ac_cpu_arch="i586"
	fi
	ac_cpu_family=5
elif grep "family.*:.*6" /proc/cpuinfo > /dev/null ; then
	if grep -i "model name.*:.*Athlon" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="athlon"
	elif grep -i "model name.*:.*Pentium IV" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentium4"
	elif grep -i "model name.*:.*Pentium III" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentium3"
	elif grep -i "model name.*:.*Pentium II" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentium2"
	elif grep -i "model name.*:.*Pentium Pro" /proc/cpuinfo > /dev/null ; then
		ac_cpu_arch="pentiumpro"
	else
		ac_cpu_arch="i686"
	fi
	ac_cpu_family=6
else
	ac_cpu_arch=blend
	ac_cpu_family=0
fi
echo "$as_me:$LINENO: checking build cpu, family" >&5
echo $ECHO_N "checking build cpu, family... $ECHO_C" >&6
echo "$as_me:$LINENO: result: $ac_cpu_arch, $ac_cpu_family" >&5
echo "${ECHO_T}$ac_cpu_arch, $ac_cpu_family" >&6


if test $ac_cpu_arch = blend; then
	ac_cc_arch=blend
else

echo "$as_me:$LINENO: checking whether ${CC-cc} accepts -march=$ac_cpu_arch" >&5
echo $ECHO_N "checking whether ${CC-cc} accepts -march=$ac_cpu_arch... $ECHO_C" >&6
echo 'void f(){}' > conftest.c
if test -z "`${CC-cc} -c -march=$ac_cpu_arch conftest.c 2>&1`"; then
  echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
